{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": ".",
		"paths": {
			"@Pages/*": ["src/pages/*"],
			"@Components/*": ["src/components/*"],
			"@Assets/*": ["assets/*"],
			"@Icons/*": ["assets/icons/*"]
		},
		"typeRoots": ["node_modules/@types", "src/types"]
	}
}
