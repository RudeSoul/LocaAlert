{
	"name": "first-rn",
	"version": "1.0.0",
	"main": "node_modules/expo/AppEntry.js",
	"scripts": {
		"start": "expo start",
		"android": "expo start --android",
		"ios": "expo start --ios",
		"web": "expo start --web",
		"prettier:base": "prettier --ignore-path .gitignore",
		"eslint:base": "eslint",
		"lint:eslint": "npm run eslint:base -- --max-warnings 0 \"**/*.{js,ts,tsx,mdx}\"",
		"lint:eslint-config-prettier": "eslint --print-config .eslintrc.json | eslint-config-prettier-check",
		"lint": "npm run lint:eslint",
		"format:prettier": "npm run prettier:base -- --write \"**/*.{js,ts,tsx,json,md,mdx}\"",
		"format:eslint": "npm run eslint:base -- --fix \"**/*.{js,ts,tsx,mdx}\"",
		"format": "npm run format:eslint && npm run format:prettier",
		"prepare": "husky install"
	},
	"dependencies": {
		"expo": "^51.0.8",
		"expo-status-bar": "^1.12.1",
		"nativewind": "^2.0.11",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-native": "^0.74.1",
		"react-native-web": "^0.19.11"
	},
	"devDependencies": {
		"@babel/core": "^7.12.9",
		"@types/react": "~18.2.79",
		"@types/react-native": "^0.69.1",
		"@typescript-eslint/eslint-plugin": "^5.36.0",
		"@typescript-eslint/parser": "^5.36.0",
		"babel-plugin-module-resolver": "^5.0.0",
		"babel-preset-expo": "~11.0.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.34.1",
		"husky": "^9.0.11",
		"postcss": "^8.4.38",
		"postcss-loader": "^8.1.1",
		"prettier": "^3.2.5",
		"tailwindcss": "3.3.2",
		"typescript": "~5.3.3"
	},
	"private": true
}
