{
	"name": "first-rn",
	"version": "1.0.0",
	"main": "expo-router/entry",
	"scripts": {
		"start": "expo start",
		"android": "expo start --android",
		"ios": "expo start --ios",
		"web": "expo start --web",
		"prettier:base": "prettier --ignore-path .gitignore",
		"eslint:base": "eslint",
		"lint:eslint": "npm run eslint:base -- --max-warnings 0 \"**/*.{js,ts,tsx,mdx}\"",
		"lint:eslint-config-prettier": "eslint --print-config .eslintrc.json | eslint-config-prettier-check",
		"lint": "npm run lint:eslint",
		"format:prettier": "npm run prettier:base -- --write \"**/*.{js,ts,tsx,json,md,mdx}\"",
		"format:eslint": "npm run eslint:base -- --fix \"**/*.{js,ts,tsx,mdx}\"",
		"format": "npm run format:eslint && npm run format:prettier",
		"prepare": "husky"
	},
	"dependencies": {
		"@react-native-community/geolocation": "^3.2.1",
		"expo": "^51.0.8",
		"expo-constants": "~16.0.1",
		"expo-linking": "~6.3.1",
		"expo-router": "~3.5.14",
		"expo-status-bar": "~1.12.1",
		"geolib": "^3.3.4",
		"lightningcss-darwin-arm64": "^1.25.0",
		"nativewind": "^2.0.11",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-native": "^0.74.1",
		"react-native-safe-area-context": "4.10.1",
		"react-native-screens": "3.31.1",
		"react-native-web": "^0.19.11"
	},
	"devDependencies": {
		"@babel/core": "^7.12.9",
		"@expo/metro-runtime": "~3.2.1",
		"@react-native-async-storage/async-storage": "^1.23.1",
		"@react-native-community/datetimepicker": "^8.0.1",
		"@react-navigation/stack": "^6.3.29",
		"@types/react": "~18.2.79",
		"@types/react-native": "^0.69.1",
		"@typescript-eslint/eslint-plugin": "^5.36.0",
		"@typescript-eslint/parser": "^5.36.0",
		"babel-plugin-module-resolver": "^5.0.0",
		"babel-preset-expo": "~11.0.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.34.1",
		"expo-checkbox": "^3.0.0",
		"expo-location": "^17.0.1",
		"expo-notifications": "^0.28.3",
		"husky": "^9.0.11",
		"postcss": "^8.4.38",
		"postcss-loader": "^8.1.1",
		"prettier": "^3.2.5",
		"react-native-google-places-autocomplete": "^2.5.6",
		"react-native-maps": "^1.15.4",
		"react-native-onboard": "^2.6.0",
		"tailwindcss": "3.3.2",
		"typescript": "~5.3.3"
	},
